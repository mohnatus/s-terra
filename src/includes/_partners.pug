.partners
  .partners-slider.swiper
    .swiper-wrapper
      -
        var partners = [
          { img: require("../img/partners/1.png"), alt: ''},
          { img: require("../img/partners/2.png"), alt: ''},
          { img: require("../img/partners/3.png"), alt: ''},
          { img: require("../img/partners/4.png"), alt: ''},
          { img: require("../img/partners/5.png"), alt: ''},
          { img: require("../img/partners/1.png"), alt: ''},
          { img: require("../img/partners/2.png"), alt: ''},
          { img: require("../img/partners/3.png"), alt: ''},
          { img: require("../img/partners/4.png"), alt: ''},
          { img: require("../img/partners/5.png"), alt: ''},
        ]

      each partner in partners
        .swiper-slide
          img(src=partner.img alt=partner.alt)

    .container
      .swiper-controls
        button.swiper-control.swiper-prev.btn.btn-circle(type="button")
          svg(width="7" height="12")
            use(xlink:href="#arrow-left" href="#arrow-left")
        button.swiper-control.swiper-next.btn.btn-circle(type="button")
          svg(width="7" height="12")
            use(xlink:href="#arrow-right" href="#arrow-right")
